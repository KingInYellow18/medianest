{
  "name": "@medianest/frontend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kinginyellow/medianest.git",
    "directory": "frontend"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:optimized": "cp next.config.performance-optimized.js next.config.js && npm run build:performance",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "analyze:performance": "node scripts/analyze-performance.js",
    "analyze:bundle": "node scripts/bundle-analysis.js",
    "analyze:full": "npm run build:analyze && npm run analyze:bundle",
    "optimize:bundle": "npm run analyze:bundle && npm run build:optimized && npm run analyze:bundle",
    "bundle:compare": "npm run build && npm run analyze:bundle && npm run build:optimized && npm run analyze:bundle",
    "build:performance": "NODE_ENV=production npm run build",
    "performance:test": "npm run build:optimized && npm run analyze:bundle"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.7",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.11.1",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.87.1",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.344.0",
    "next": "15.5.2",
    "next-auth": "^4.24.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.52.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.57.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@next/eslint-plugin-next": "15.5.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.14.10",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^3.0.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^13.24.0",
    "jsdom": "^24.0.0",
    "msw": "^2.11.1",
    "postcss": "^8.4.40",
    "prettier": "^3.2.5",
    "prisma": "^6.15.0",
    "tailwindcss": "^3.4.1",
    "types-react-codemod": "^3.5.2",
    "typescript": "^5.5.3",
    "vitest": "^3.2.4"
  }
}
