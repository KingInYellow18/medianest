{
  "name": "@medianest/frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "analyze:performance": "node scripts/analyze-performance.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "visual-test": "npm run build-storybook && npm run chromatic",
    "visual-test:ci": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.4",
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^6.11.1",
    "@tanstack/react-query": "^5.85.6",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.3",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.344.0",
    "next": "14.2.30",
    "next-auth": "^4.24.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.30",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/react": "^9.0.17",
    "@storybook/react-vite": "^9.0.17",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "chromatic": "^13.1.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.30",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.40",
    "prisma": "^6.11.1",
    "storybook": "^9.0.17",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
